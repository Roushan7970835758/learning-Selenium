<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="LambdaTestSuite" parallel="tests" thread-count="2">
    <test name="Chrome Windows 10">
        <parameter name="browser" value="chrome"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="Practice.LambdaTestAutomation"/>
        </classes>
    </test>
    <test name="Edge macOS Ventura">
        <parameter name="browser" value="MicrosoftEdge"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="macOS Ventura"/>
        <classes>
            <class name="Practice.LambdaTestAutomation"/>
        </classes>
    </test>
</suite>

